<%- model_class = ProductosVendido -%>
<div class="page-header">
  <h1>Producto Agregado a Orden</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @productos_vendido.producto.nombre %></dd>
</dl>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:cantidad) %>:</strong></dt>
  <dd><%= @productos_vendido.cantidad %></dd>
</dl>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:especificaciones) %>:</strong></dt>
  <dd><%= @productos_vendido.especificaciones %></dd>
</dl>

<div class="form-actions">
  <%= link_to "AtrÃ¡s",
              productos_selProductos_path, :class => 'btn'  %>
  <%= link_to "Editar",
              edit_productos_vendido_path(@productos_vendido), :class => 'btn' %>
  <%= link_to "Eliminar",
              productos_vendido_path(@productos_vendido),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

